{
	"name": "simulacrum-svelte",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"npm": ">=7.14.0",
		"node": ">=16.14.2"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"start": "node build/index.js",
		"rs:build": "cargo build --release --target wasm32-unknown-unknown",
		"rs:wasm": "wasm-bindgen target/wasm32-unknown-unknown/release/game.wasm --out-dir wasm --no-modules"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.89",
		"@sveltejs/kit": "next",
		"css-tree": "^2.3.1",
		"mdsvex": "^0.10.6",
		"rehype-katex-svelte": "^1.1.2",
		"remark-math": "^3.0.0",
		"sass": "^1.54.9",
		"svelte": "^3.55.1",
		"svelte-check": "^2.7.1",
		"svelte-motion": "^0.11.5",
		"svelte-preprocess": "^4.10.7",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"@atomictech/rehype-toc": "^3.1.3",
		"@motion-canvas/2d": "^2.3.0",
		"@motion-canvas/core": "^2.3.0",
		"@motion-canvas/player": "^2.3.0",
		"@motion-canvas/ui": "^2.3.0",
		"@motion-canvas/vite-plugin": "^2.2.0",
		"@svelte-use/core": "^0.11.0",
		"@types/animejs": "^3.1.7",
		"@types/d3": "^7.4.0",
		"@types/three": "^0.149.0",
		"@types/uuid": "^9.0.0",
		"animejs": "^3.2.1",
		"axios": "^1.3.4",
		"d3": "^7.8.2",
		"mathlifier": "^1.3.4",
		"node-fetch": "^3.3.0",
		"prismjs": "^1.29.0",
		"rehype-autolink-headings": "^6.1.1",
		"rehype-document": "^6.1.0",
		"rehype-katex": "^6.0.2",
		"rehype-parse": "^8.0.4",
		"rehype-slug": "^5.1.0",
		"rehype-stringify": "^9.0.3",
		"remark": "^14.0.2",
		"remark-toc": "^8.0.1",
		"svelte-katex": "^0.1.2",
		"svelte-use": "^0.0.1",
		"svelthree": "^0.119.0-5",
		"three": "^0.149.0",
		"unified": "^10.1.2",
		"uuid": "^9.0.0"
	}
}
